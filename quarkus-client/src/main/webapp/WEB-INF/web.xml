<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" id="WebApp_ID"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	
	<display-name>quarkus-client</display-name>
	
	<listener>
		<listener-class>org.jboss.weld.environment.servlet.Listener</listener-class>
	</listener>
	
	<servlet>
		<servlet-name>PersonaClienteServlet</servlet-name>
		<servlet-class>PersonaClienteServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>PersonaClienteServlet</servlet-name>
		<url-pattern>/quarkus-client/PersonaClienteServlet</url-pattern>
	</servlet-mapping>
	
	
	<welcome-file-list>
		<welcome-file>"/"</welcome-file>
	</welcome-file-list>
	
</web-app>